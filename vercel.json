{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "rewrites": [
    {
      "source": "/:any*",
      "destination": "/"
    }
  ]
}
