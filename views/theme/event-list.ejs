    <script>
      function loadAllContent() {
        var allContent = document.getElementById('all-content');
        var ongoingContent = document.getElementById('ongoing').querySelectorAll('.col-xxl-4.col-md-6');
        var doingContent = document.getElementById('doing').querySelectorAll('.col-xxl-4.col-md-6');
        var doneContent = document.getElementById('done').querySelectorAll('.col-xxl-4.col-md-6');

        function duplicateContents(contents) {
          for (var i = 0; i < contents.length; i++) {
            var duplicate = contents[i].cloneNode(true);
            allContent.appendChild(duplicate);
          }
        }

        duplicateContents(ongoingContent);
        duplicateContents(doingContent);
        duplicateContents(doneContent);
      }

      window.addEventListener('DOMContentLoaded', loadAllContent);
